{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/lib/**",
                "~/.conan/data/**",
                "/usr/include/qt/**"
            ],
            "defines": [
                "NODE_EDITOR_SHARED",
